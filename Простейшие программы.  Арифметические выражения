    return 0;
}
1.3
#include <iostream>

int main() {
    int number;
    std::cout << "Введите число: ";
    std::cin >> number;
    std::cout << "Вы ввели число " << number << std::endl;
    return 0;
}
1.4
#include <iostream>

int main() {
    int number;
    std::cout << "Введите число: ";
    std::cin >> number;
    std::cout << number << " вот какое число Вы ввели" << std::endl;
    return 0;
}
