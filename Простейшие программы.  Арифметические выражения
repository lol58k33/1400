1.7
#include <iostream>

int main() {
    int num1, num2, num3;
    std::cout << "Введите три числа: ";
    std::cin >> num1 >> num2 >> num3;
    std::cout << num1 << "  " << num2 << "  " << num3 << std::endl;
    return 0;
}
1.8
#include <iostream>

int main() {
    int num1, num2, num3, num4;
    std::cout << "Введите четыре числа: ";
    std::cin >> num1 >> num2 >> num3 >> num4;
    std::cout << num1 << " " << num2 << " " << num3 << " " << num4 << std::endl;
    return 0;
}
