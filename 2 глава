void task2_18() {
    cout << "\n2.18. Все перестановки цифр трехзначного числа" << endl;
    int n;
    cout << "Введите трехзначное число: ";
    cin >> n;
    
    int a = n / 100;
    int b = (n / 10) % 10;
    int c = n % 10;
    
    cout << "Все перестановки:" << endl;
    cout << a << b << c << endl;
    cout << a << c << b << endl;
    cout << b << a << c << endl;
    cout << b << c << a << endl;
    cout << c << a << b << endl;
    cout << c << b << a << endl;
}

void task2_19() {
    cout << "\n2.19. Четырехзначное число - сумма и произведение цифр" << endl;
    int n;
    cout << "Введите четырехзначное число: ";
    cin >> n;
    
    int d1 = n / 1000;
    int d2 = (n / 100) % 10;
    int d3 = (n / 10) % 10;
    int d4 = n % 10;
    
    int sum = d1 + d2 + d3 + d4;
    int product = d1 * d2 * d3 * d4;
    
    cout << "а) Сумма цифр: " << sum << endl;
    cout << "б) Произведение цифр:  " << product << endl;
}

