void task2_10() {
    cout << "\n2.10. Двузначное число - работа с цифрами" << endl;
    int n;
    cout << "Введите двузначное число: ";
    cin >> n;
    
    int tens = n / 10;
    int units = n % 10;
    int sum = tens + units;
    int product = tens * units;
    
    cout << "а) Число десятков: " << tens << endl;
    cout << "б) Число единиц: " << units << endl;
    cout << "в) Сумма цифр: " << sum << endl;
    cout << "г) Произведение цифр: " << product << endl;
}

void task2_11() {
    cout << "\n2.11. Перестановка цифр двузначного числа" << endl;
    int n;
    cout << "Введите двузначное число: ";
    cin >> n;
    
    int tens = n / 10;
    int units = n % 10;
    int result = units * 10 + tens;
    
    cout << "Результат: " << result << endl;
}

void task2_12() {
    cout << "\n2.12. Трехзначное число - работа с цифрами" << endl;
    int n;
    cout << "Введите трехзначное число: ";
    cin >> n;
    
    int hundreds = n / 100;
    int tens = (n / 10) % 10;
    int units = n % 10;
    int sum = hundreds + tens + units;
    int product = hundreds * tens * units;
    
    cout << "а) Число единиц: " << units << endl;
    cout << "б) Число десятков: " << tens << endl;
    cout << "в) Сумма цифр: " << sum << endl;
    cout << "г) Произведение цифр:  " << product << endl;
}

void task2_13() {
    cout << "\n2.13. Прочитать трехзначное число справа налево" << endl;
    int n;
    cout << "Введите трехзначное число: ";
    cin >> n;
    
    int hundreds = n / 100;
    int tens = (n / 10) % 10;
    int units = n % 10;
    int result = units * 100 + tens * 10 + hundreds;
    
    cout << "Результат:  " << result << endl;
}

void task2_14() {
    cout << "\n2.14. Первую цифру перенести в конец" << endl;
    int n;
    cout << "Введите трехзначное число: ";
    cin >> n;
    
    int hundreds = n / 100;
    int remainder = n % 100;
    int result = remainder * 10 + hundreds;
    
    cout << "Результат: " << result << endl;
}

