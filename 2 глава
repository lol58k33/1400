    cin >> n;
    
    int d1 = n / 1000;
    int d2 = (n / 100) % 10;
    int d3 = (n / 10) % 10;
    int d4 = n % 10;
    
    int result = d4 * 1000 + d3 * 100 + d2 * 10 + d1;
    cout << "Результат: " << result << endl;
}

