void task2_1() {
    cout << "\n2.1. Расстояние в сантиметрах -> метры" << endl;
    int cm;
    cout << "Введите расстояние (см): ";
    cin >> cm;
    cout << "Полных метров: " << cm / 100 << endl;
}

