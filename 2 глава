// ============================================
// ВОССТАНОВЛЕНИЕ ЧИСЛА ПО ЦИФРАМ (2.24 - 2.33)
// ============================================

void task2_24() {
    cout << "\n2.24. Восстановить число x по условиям" << endl;
    // (x - x%10) / 10 -> слева приписать x%10 -> 237
    // x/10 с приписанной слева последней цифрой = 237
    // Если последняя цифра c, то c*100 + x/10 = 237
    // Значит x/10 = 37, c = 2
    // x = 372
    
    int n = 237;
    int c = n / 100;  // последняя цифра исходного числа
    int xy = n % 100; // первые две цифры исходного числа (x/10)
    int x = xy * 10 + c;
    
    cout << "Результат: x = " << x << endl;
}

void task2_25() {
    cout << "\n2.25. По заданному n найти x" << endl;
    int n;
    cout << "Введите число n (10 ≤ n ≤ 999): ";
    cin >> n;
    
    int c = n / 100;  // последняя цифра исходного числа
    int xy = n % 100; // первые две цифры исходного числа
    int x = xy * 10 + c;
    
    cout << "x = " << x << endl;
}

void task2_26() {
    cout << "\n2.26. Восстановить x по условиям" << endl;
    // Зачеркнули первую цифру, остаток * 10 + первая = 564
    // a*100 + bc
    // bc*10 + a = 564
    // Значит:  a = 5, bc*10 = 560, bc = 56
    // x = 556
    
    int n = 564;
    int a = n / 100;
    int bc = n % 100 / 10;
    int x = a * 100 + bc * 10 + (n % 10);
    
    // Правильная интерпретация: 
    // bc*10 + a = 564
    // Если a = 4, то bc*10 = 560, bc = 56
    int a_correct = n % 10;
    int bc_correct = (n / 10) % 10 * 10 + n / 100;
    int x_correct = a_correct * 100 + bc_correct;
    
    cout << "Результат: x = " << x_correct << endl;
}

void task2_27() {
    cout << "\n2.27. По заданному n найти x" << endl;
    int n;
    cout << "Введите число n (1 ≤ n ≤ 999): ";
    cin >> n;
    
    // bc*10 + a = n
    // Ищем a (последняя цифра исходного)
    // Ищем bc (две первые цифры)
    
    if (n < 10) {
        cout << "x = " << n * 10 << endl; // a = n, bc = 0
    } else if (n < 100) {
        int a = n % 10;
        int b = n / 10;
        cout << "x = " << a * 100 + b * 10 << endl;
    } else {
        int a = n % 10;
        int bc = n / 10;
        cout << "x = " << a * 100 + bc << endl;
    }
}

void task2_28() {
    cout << "\n2.28. Восстановить x по условиям" << endl;
    // Зачеркнули вторую цифру
    // К ac слева приписали b = 546
    // b*100 + ac = 546
    // b = 5, ac = 46
    // x = 5*100 + 4*10 + 6 = 546 (неправильно)
    // ac = 46 -> a = 4, c = 6
    // x = a*100 + b*10 + c = 4*100 + 5*10 + 6 = 456
    
    int n = 546;
    int b = n / 100;
    int ac = n % 100;
    int a = ac / 10;
    int c = ac % 10;
    int x = a * 100 + b * 10 + c;
    
    cout << "Результат: x = " << x << endl;
}

void task2_29() {
    cout << "\n2.29. По заданному n найти x" << endl;
    int n;
    cout << "Введите число n (10 ≤ n ≤ 999): ";
    cin >> n;
    
    int b = n / 100;
    int ac = n % 100;
    int a = ac / 10;
    int c = ac % 10;
    int x = a * 100 + b * 10 + c;
    
    cout << "x = " << x << endl;
}

void task2_30() {
    cout << "\n2.30. Восстановить x по условиям" << endl;
    // Зачеркнули вторую цифру
    // К ac справа приписали b = 456
    // ac*10 + b = 456
    // ac = 45, b = 6
    // a = 4, c = 5
    // x = 4*100 + 6*10 + 5 = 465
    
    int n = 456;
    int ac = n / 10;
    int b = n % 10;
    int a = ac / 10;
    int c = ac % 10;
    int x = a * 100 + b * 10 + c;
    
    cout << "Результат: x = " << x << endl;
}

