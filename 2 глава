    int x = a * 100 + b * 10 + c;
    
    cout << "x = " << x << endl;
}

void task2_32() {
    cout << "\n2.32. Восстановить x по условиям" << endl;
    // Зачеркнули последнюю цифру -> ab
    // Переставили цифры в ab -> ba
    // Приписали c слева -> c*100 + ba = 654
    // c = 6, ba = 54 -> b = 5, a = 4
    // x = 456
    
    int n = 654;
    int c = n / 100;
    int ba = n % 100;
    int b = ba / 10;
    int a = ba % 10;
    int x = a * 100 + b * 10 + c;
    
    cout << "Результат: x = " << x << endl;
}

void task2_33() {
    cout << "\n2.33. По заданному n найти x" << endl;
    int n;
    cout << "Введите число n (1 ≤ n ≤ 999): ";
    cin >> n;
    
    int c = n / 100;
    int ba = n % 100;
    int b = ba / 10;
    int a = ba % 10;
    int x = a * 100 + b * 10 + c;
    
    cout << "x = " << x << endl;
}

