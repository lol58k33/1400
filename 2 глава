void task2_34() {
    cout << "\n2.34. Сумма двузначного и однозначного (без условных операторов)" << endl;
    int a1, a2, b;
    cout << "Введите десятки и единицы первого числа, и однозначное число: ";
    cin >> a1 >> a2 >> b;
    
    int sum = a1 * 10 + a2 + b;
    int result_tens = sum / 10;
    int result_units = sum % 10;
    
    cout << "Цифры результата: " << result_tens << " " << result_units << endl;
}

void task2_35() {
    cout << "\n2.35. Сумма двух двузначных (без условных операторов)" << endl;
    int a1, a2, b1, b2;
    cout << "Введите десятки и единицы первого числа, десятки и единицы второго:  ";
    cin >> a1 >> a2 >> b1 >> b2;
    
    int sum = (a1 * 10 + a2) + (b1 * 10 + b2);
    int result_tens = sum / 10;
    int result_units = sum % 10;
    
    cout << "Цифры результата: " << result_tens << " " << result_units << endl;
}

void task2_36() {
    cout << "\n2.36. Сумма трехзначного и двузначного (без условных операторов)" << endl;
    int a1, a2, a3, b1, b2;
    cout << "Введите сотни, десятки, единицы первого и десятки, единицы второго:  ";
    cin >> a1 >> a2 >> a3 >> b1 >> b2;
    
    int sum = (a1 * 100 + a2 * 10 + a3) + (b1 * 10 + b2);
    int result_hundreds = sum / 100;
    int result_tens = (sum / 10) % 10;
    int result_units = sum % 10;
    
    cout << "Цифры результата: " << result_hundreds << " " << result_tens << " " << result_units << endl;
}

void task2_37() {
    cout << "\n2.37. Последовательность 101112131415... 9899" << endl;
    int k;
    cout << "Введите номер цифры (1-180): ";
    cin >> k;
    
    int pair_number = (k - 1) / 2 + 1;
    int two_digit_number = 10 + pair_number - 1;
    int digit_position = (k - 1) % 2;
    
    int digit;
    if (digit_position == 0) {
        digit = two_digit_number / 10;
    } else {
        digit = two_digit_number % 10;
    }
    
    cout << "а) Номер пары: " << pair_number << endl;
    cout << "б) Двузначное число: " << two_digit_number << endl;
    cout << "в) k-я цифра: " << digit << endl;
}

void task2_38() {
    cout << "\n2.38. Последовательность 101102103... 149150" << endl;
    int k;
    cout << "Введите номер цифры (1-150): ";
    cin >> k;
    
    int number_index = (k - 1) / 3;
    int digit_position = (k - 1) % 3;
    int three_digit_number = 101 + number_index;
    
    int digit;
    if (digit_position == 0) {
        digit = three_digit_number / 100;
    } else if (digit_position == 1) {
        digit = (three_digit_number / 10) % 10;
    } else {
        digit = three_digit_number % 10;
    }
    
    cout << "k-я цифра: " << digit << endl;
}

void task2_39() {
    cout << "\n2.39. Угол часовой стрелки" << endl;
    int h, m, s;
    cout << "Введите часы, минуты, секунды:  ";
    cin >> h >> m >> s;
    
    double angle = h * 30. 0 + m * 0.5 + s * (0.5 / 60.0);
    cout << fixed << setprecision(2);
    cout << "Угол часовой стрелки:  " << angle << " градусов" << endl;
}

void task2_40() {
    cout << "\n2.40. По углу часовой стрелки найти время" << endl;
    double y;
    cout << "Введите угол (0 <= y < 360): ";
    cin >> y;
    
    int hours = (int)(y / 30);
    int minutes = (int)((y - hours * 30) / 0.5);
    
    cout << "Часов: " << hours << ", Минут: " << minutes << endl;
}

void task2_41() {
    cout << "\n2.41. Угол часовой стрелки в радианах" << endl;
    double y;
    cout << "Введите угол в радианах (0 < y <= 2π): ";
    cin >> y;
    
    double minutes_angle = (y / (2 * M_PI)) * 360. 0;
    int hours = (int)(y / (M_PI / 6));
    int minutes = (int)((y - hours * M_PI / 6) / (M_PI / 360. 0));
    
    cout << "Угол минутной стрелки: " << minutes_angle << " градусов" << endl;
    cout << "Часов: " << hours << ", Минут:  " << minutes << endl;
}

void task2_42a() {
    cout << "\n2.42a. Время до совпадения стрелок" << endl;
    int h, m;
    cout << "Введите часы и минуты (1 <= h <= 12, 0 <= m <= 59): ";
    cin >> h >> m;
    
    // Угол часовой стрелки:  30h + 0.5m
    // Угол минутной стрелки: 6m
    // Совпадение: 6m ≡ 30h + 0.5m (mod 360)
    // 5. 5m ≡ 30h (mod 360)
    // m ≡ 30h/5.5 (mod 360/5.5)
    
    // Проще: находим для каждого часа время совпадения
    int minute_of_coincidence = (int)(h * 60 / 11) % 60;
    int time_to_wait = minute_of_coincidence - m;
    if (time_to_wait <= 0) time_to_wait += 720 / 11;
    
    cout << "Минут до совпадения: " << time_to_wait << endl;
}

void task2_42b() {
    cout << "\n2.42б. Время до перпендикулярности стрелок" << endl;
    int h, m;
    cout << "Введите часы и минуты:  ";
    cin >> h >> m;
    
    cout << "Минут до перпендикулярности: " << (15 - m) << endl;
}

void task2_43() {
    cout << "\n2.43. Делимость без условных операторов" << endl;
    int a, b;
    cout << "Введите два числа:  ";
    cin >> a >> b;
    
    int result = (a % b == 0 || b % a == 0) ? 1 : 0;
    cout << "Результат: " << result << endl;
}

