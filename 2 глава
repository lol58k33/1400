    cout << "\n2.5. 234 дня -> недели" << endl;
    int days = 234;
    cout << "Дано " << days << " дней" << endl;
    cout << "Полных недель: " << days / 7 << endl;
}

void task2_6() {
    cout << "\n2.6. Время в секундах" << endl;
    int n;
    cout << "Введите секунды с начала суток: ";
    cin >> n;
    
    int hours = n / 3600;
    int minutes_in_hour = (n % 3600) / 60;
    int seconds_in_minute = n % 60;
    
    cout << "а) Полных часов: " << hours << endl;
    cout << "б) Полных минут в текущем часе: " << minutes_in_hour << endl;
    cout << "в) Полных секунд в текущей минуте: " << seconds_in_minute << endl;
}

void task2_7() {
    cout << "\n2.7. Квадраты из прямоугольника" << endl;
    int length = 543, width = 130, side = 130;
    cout << "Размеры прямоугольника:  " << length << " x " << width << " мм" << endl;
    cout << "Сторона квадрата: " << side << " мм" << endl;
    int squares = (length / side) * (width / side);
    cout << "Количество квадратов: " << squares << endl;
}

void task2_8a() {
    cout << "\n2.8a. День н��дели (если 1 янв - понедельник)" << endl;
    int k;
    cout << "Введите номер дня года (1-365): ";
    cin >> k;
    int weekday = (k - 1) % 7 + 1;
    cout << "День недели: " << weekday << " (1=Пн, 2=Вт, ..., 7=Вс)" << endl;
}

void task2_8b() {
    cout << "\n2.8б. День недели (если 1 янв - вторник)" << endl;
    int k;
    cout << "Введите номер дня года (1-365): ";
    cin >> k;
    int weekday = (k - 1 + 1) % 7 + 1;
    cout << "День недели: " << weekday << " (1=Пн, 2=Вт, ..., 7=Вс)" << endl;
}

void task2_8c() {
    cout << "\n2.8в. День недели (d-й день недели для 1 янв)" << endl;
    int k, d;
    cout << "Введите номер дня года (1-365): ";
    cin >> k;
    cout << "Введите день недели для 1 января (1-7): ";
    cin >> d;
    int weekday = (k - 1 + d - 1) % 7 + 1;
    cout << "День недели: " << weekday << endl;
}

void task2_9() {
    cout << "\n2.9. Месяц от начала 1990 года" << endl;
    int n;
    cout << "Введите количество полных месяцев: ";
    cin >> n;
    int month = (n + 1);
    if (month > 12) month = month % 12;
    if (month == 0) month = 12;
    cout << "Месяц: " << month << " (1=Янв, .. ., 12=Дек)" << endl;
}

