void task2_20g_method1() {
    cout << "\n2.20г (Способ 1). Перестановка (1,2) и (3,4) пар - с выделением цифр" << endl;
    int n;
    cout << "Введите четырехзначное число: ";
    cin >> n;
    
    int d1 = n / 1000;
    int d2 = (n / 100) % 10;
    int d3 = (n / 10) % 10;
    int d4 = n % 10;
    
    int result = d3 * 1000 + d4 * 100 + d1 * 10 + d2;
    cout << "Результат: " << result << endl;
}

void task2_20g_method2() {
    cout << "\n2.20г (Способ 2). Перестановка пар - без выделения цифр" << endl;
    int n;
    cout << "Введите четырехзначное число: ";
    cin >> n;
    
    int prefix = n / 100;   // первые две цифры
    int suffix = n % 100;   // последние две цифры
    
    int result = suffix * 100 + prefix;
    cout << "Результат: " << result << endl;
}

